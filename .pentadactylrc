group user
map -m n,v,i,c,t <C-a> <Pass>
map -m n,v,i,c,t <C-c> <Pass>
map -m i,c,t <C-v> <Pass>
map -m i,c,t <C-x> <Pass>
map -m i,c,t <C-z> <Pass>
map -count -modes=n h <count>gT
map -count -modes=n l <count>gt
map -count -modes=n k <count><C-u>
map -count -modes=n j <count><C-d>
